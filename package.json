{
  "name": "kalkal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:production": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",

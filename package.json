    "deploy:production": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:script": "powershell -ExecutionPolicy Bypass -File scripts/deploy-vercel.ps1",
    "deploy:check": "node scripts/check-production-ready.js",
    "logs:production": "vercel logs --follow",
    "backup:db": "bash scripts/backup-database.sh",
